{
    "name": "carfify/v4.0",
    "description": "Carfify Enterprise Application v4.0",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Carfify AI",
            "email": "dev@carfify.com"
        }
    ],
    "require": {
        "php": ">=7.4"
    },
    "autoload": {
        "psr-4": {
            "Core\\": "core/",
            "App\\": "src/"
        },
        "files": [
            "core/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    }
}